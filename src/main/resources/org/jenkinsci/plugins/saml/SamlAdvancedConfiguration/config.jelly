<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core"
         xmlns:f="/lib/form">
    <f:entry title="Force Authentication" field="forceAuthn" help="/plugin/saml/help/forceAuthn.html">
        <f:checkbox/>
    </f:entry>
    <f:entry title="Authentication Context" field="authnContextClassRef"
             help="/plugin/saml/help/authnContextClassRef.html">
        <f:textbox/>
    </f:entry>
    <f:entry title="SP Entity ID" field="spEntityId" help="/plugin/saml/help/spEntityId.html">
        <f:textbox/>
    </f:entry>
    <f:entry title="Maximum Session Lifetime" field="maximumSessionLifetime"
             help="/plugin/saml/help/maximumSessionLifetime.html">
        <f:number clazz="required number" min="0" default="86400"/>
    </f:entry>
    <f:entry title="Group Pattern" field="groupRegexPattern" help="/plugin/saml/help/groupRegexPattern.html">
        <f:textbox default="CN=([^,]+),.*"/>
    </f:entry>
    <f:entry title="Group Replace" field="groupRegexReplace" help="/plugin/saml/help/groupRegexReplace.html">
        <f:textbox default="$1"/>
    </f:entry>
</j:jelly>
